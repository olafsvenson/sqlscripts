<?xml version="1.0" encoding="utf-8"?>
<Snippet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>tempdb</Name>
  <Description>top tempdb queries</Description>
  <Text>exec sp_whoisactive
@show_sleeping_spids=0,
@sort_order = '[tempdb_current] desc',
@output_column_list = '[tempdb_current][dd%][session_id][status][sql_text][host_name][program_name][login_name][wait_info]';</Text>
</Snippet>